{
  "project": "Accenture Newsroom",
  "plugins": [
    {
      "id": "tagger",
      "title": "Tagger",
      "environments": [
        "edit"
      ],
      "url": "/tools/tagger"
    },
    {
      "id": "preflight",
      "title": "Preflight",
      "environments": [
        "dev", "preview", "live"
      ],
      "event": "preflight"
    },
    {
      "id": "library",
      "title": "Library",
      "environments": [ "edit" ],
      "url": "/tools/sidekick/library",
      "includePaths": [ "**.docx**" ]
    }
  ],
  "specialViews": [{
    "title": "PDF Viewer",
    "path": "**.pdf",
    "viewer": "/tools/pdf-viewer/index.html"
  }]
}